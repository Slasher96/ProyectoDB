@model MagicalLogger.Models.UsuariosModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es-mx">
<head>
    <title>Usarios Existentes</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/perfect-scrollbar/perfect-scrollbar.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/CSS/util.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/CSS/main.css")">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-table100">
            <div class="wrap-table100">
                <div class="table">

                    <div class="row header">
                        <div class="cell">
                            Id Usuario
                        </div>
                        <div class="cell">
                            Nombre usuario
                        </div>
                        <div class="cell">
                            Nombre
                        </div>
                        <div class="cell">
                            Primer apellido
                        </div>
                        <div class="cell">
                            Segundo apellido
                        </div>
                        <div class="cell">
                            Numero INE
                        </div>
                        <div class="cell">
                            teléfono
                        </div>
                        <div class="cell">
                            Id Perfil
                        </div>
                        <div class="cell">
                            Acciones
                        </div>
                    </div>
                    @foreach (var item in Model.ListaUsuarios)
                    {
                        <div class="row">
                            <div class="cell">
                                @item.IdUsuario
                            </div>
                            <div class="cell">
                                @item.NombreUsuario
                            </div>
                            <div class="cell">
                                @item.Nombre
                            </div>
                            <div class="cell">
                                @item.PrimerApellido
                            </div>
                            <div class="cell">
                                @item.SegundoApellido
                            </div>
                            <div class="cell">
                                @item.NumeroIne
                            </div>
                            <div class="cell">
                                @item.Telefono
                            </div>
                            <div class="cell">
                                @item.IdPerfil
                            </div>
                            <div class="cell">
                               <a href="@Url.Action("deleteuser","Main")">Eliminar Usuario</a>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>




    <!--===============================================================================================-->
    <script src="vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/bootstrap/js/popper.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="js/main.js"></script>

</body>
</html>